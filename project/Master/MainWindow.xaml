<Window x:Class="TimeMiner.Master.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TimeMiner.Master"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525" Closing="Window_Closing" Loaded="Window_Loaded">
    <Grid>
        <Button x:Name="button" Content="Go timeTo admin page" HorizontalAlignment="Left" Margin="26,22,0,0" VerticalAlignment="Top" Width="128" Click="button_Click" Height="34"/>
        <Button x:Name="btClearCache" Content="Clear cache" HorizontalAlignment="Left" Margin="26,75,0,0" VerticalAlignment="Top" Width="127" Height="38" Click="btClearData_Click"/>
        <Button x:Name="btUnloadCollections" Content="Unload collections" HorizontalAlignment="Left" Margin="26,130,0,0" VerticalAlignment="Top" Width="127" Height="38" Click="btUnloadCollections_Click"/>

    </Grid>
</Window>
